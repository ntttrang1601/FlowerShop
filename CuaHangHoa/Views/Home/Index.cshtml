    <!-- Slider/Intro Section Start -->
    <div class="intro11-slider-wrap section-2 mrl-50">
        <div class="intro11-slider swiper-container">
            <div class="swiper-wrapper">
                <div class="intro11-section swiper-slide slide-4 slide-bg-2 bg-position">
                    <!-- Intro Content Start -->
                    <div class="intro11-content-2 text-center">
                        <h1 class="different-title">FloSun xin chào</h1>
                        <h5 class="title">Tinh tế trong từng cánh hoa</h5>
                        <a href="@Url.Action("Index","Product")" class="btn flosun-button secondary-btn theme-color rounded-0">Mua sắm ngay</a>
                    </div>
                    <!-- Intro Content End -->
                </div>
                <div class="intro11-section swiper-slide slide-3 slide-bg-2 bg-position">
                    <!-- Intro Content Start -->
                    <div class="intro11-content text-left">
                        <h3 class="title-slider text-uppercase">Tinh tế trong từng cánh hoa</h3>
                        <h2 class="title">Hoa tươi <br> Dịch vụ hoa</h2>
                        <p class="desc-content">Trong hành trình phát triển, shop hoa tươi không tránh khỏi những thách thức. 
                            Đối mặt với sự cạnh tranh ngày càng khốc liệt, các cửa hàng phải đổi mới để giữ chân khách hàng. 
                            Cơ hội xuất phát từ sự sáng tạo trong thiết kế, mở rộng dịch vụ, và tận dụng công nghệ để tạo ra trải nghiệm mua sắm hoa tươi độc đáo. </p>
                        <a href="@Url.Action("Index","Product")" class="btn flosun-button secondary-btn rounded-0">Mua sắm ngay</a>
                    </div>
                    <!-- Intro Content End -->
                </div>
            </div>
            <!-- Slider Navigation -->
            <div class="home1-slider-prev swiper-button-prev main-slider-nav"><i class="lnr lnr-arrow-left"></i></div>
            <div class="home1-slider-next swiper-button-next main-slider-nav"><i class="lnr lnr-arrow-right"></i></div>
            <!-- Slider pagination -->
            <div class="swiper-pagination"></div>
        </div>
    </div>
    <!-- Slider/Intro Section End -->
    
    <!--Product Area Start-->
    <!-- Shop Main Area Start Here -->
   
    <div class="shop-main-area">
        <div class="container container-default custom-area">
            <div class="row flex-row-reverse">
                <div class="col-12 col-custom widget-mt">
                    <div class="row">
                        <!--Section Title Start-->
                        <div class="col-12 col-custom">
                            <div class="section-title text-center mb-30">
                                <span class="section-title-1">Món quà của trái tim</span>
                                <h3 class="section-title-2">Sản phẩm nổi bật</h3>
                            </div>
                        </div>
                        <!--Section Title End-->
                    </div>
                    <!-- Shop Wrapper Start -->
                    <div class="row shop_wrapper grid_4">
                        @foreach (var product in Model.HinhCuaSPs)
                        {
                            <div class="col-lg-3 col-md-6 col-sm-6  col-custom product-area">
                                <div class="product-item">
                                    <div class="single-product position-relative mr-0 ml-0">
                                        <div class="product-image">
                                            <a class="d-block" href="@Url.Action("ProductDetail", "Product", new { id = product.SanPham.Id })">
                                                <img src="~/images/SanPham/@Url.Content(product.Url)" alt="" class="product-image-1 w-100" width="300px" height="350px">
                                            </a>
                                            <div class="add-action d-flex flex-column position-absolute">                                   
                                                <a href="@Url.Action("ProductDetail", "Product", new { id = product.SanPham.Id })" title="Xem chi tiết">
                                                    <i class="lnr lnr-eye" data-toggle="tooltip" data-placement="left" title="Xem chi tiết"></i>
                                                </a>

                                            </div>
                                        </div>
                                        <div class="product-content">
                                            <div class="product-title">
                                                <h4 class="title-2"> <a href="product-details.html">@product.SanPham.Ten</a></h4>
                                            </div>
                                            @* <div class="product-rating">
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star-o"></i>
                                                <i class="fa fa-star-o"></i>
                                            </div> *@
                                            <div class="price-box">
                                                @if (product.SanPham.PhanTramGiamGia > 0)
                                                {
                                                    <span class="regular-price text-decoration-line-through text-muted">
                                                        @product.SanPham.Dongia.ToString("#,##") đ
                                                    </span>
                                                    <span class="sale-price text-danger">
                                                        @(double.Round(product.SanPham.Dongia * (1 - (double)product.SanPham.PhanTramGiamGia / 100), 0).ToString("#,##") + " đ")
                                                    </span>
                                                }
                                                else
                                                {
                                                    <span class="regular-price">@product.SanPham.Dongia.ToString("#,##") đ</span>
                                                }
                                                @* <span class="regular-price ">@product.SanPham.Dongia.ToString("#,##") đ</span> *@
                                            </div>
                                            <a href="@Url.Action("ProductDetail", "Product", new { id = product.SanPham.Id })" class="btn product-cart">Thêm vào giỏ hàng</a>
                                        </div>
                                    </div>                                    
                                </div>
                            </div>
                        }
                    </div>
                    <!-- Shop Wrapper End -->
                </div>
            </div>
        </div>
    </div>
    <!--Categories Area Start-->
    <div class="categories-area mt-no-text">
        <div class="container custom-area">
